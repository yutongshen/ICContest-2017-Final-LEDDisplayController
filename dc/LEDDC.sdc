set_output_delay -clock GCK  -min 0  [get_ports {OUT[8]}]
set_output_delay -clock GCK  -max 1  [get_ports {OUT[7]}]
set_output_delay -clock GCK  -min 0  [get_ports {OUT[7]}]
set_output_delay -clock GCK  -max 1  [get_ports {OUT[6]}]
set_output_delay -clock GCK  -min 0  [get_ports {OUT[6]}]
set_output_delay -clock GCK  -max 1  [get_ports {OUT[5]}]
set_output_delay -clock GCK  -min 0  [get_ports {OUT[5]}]
set_output_delay -clock GCK  -max 1  [get_ports {OUT[4]}]
